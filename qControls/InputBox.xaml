<Window x:Class="qControls.InputBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:qControls"
        mc:Ignorable="d"
        Title="InputBox" Height="97.594" Width="396" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded" WindowStyle="ToolWindow">
    <Grid Margin="0,0,-0.4,0.4">


        <WrapPanel Name="Panel" Margin="0,1,0,30.8" Orientation="Vertical">
        </WrapPanel>
        <Button Name="btnCancel" Content="_Cancel" Margin="0,0,95,5" Width="74"  Height="26" 
                VerticalAlignment="Bottom" HorizontalAlignment="Right"  Click="btnCancel_Click"/>
        <Button Name="btnAccept" Content="_Accept" Margin="0,0,13,5" Width="74"  Height="26" 
                VerticalAlignment="Bottom" HorizontalAlignment="Right" Click="btnAccept_Click"/>
    </Grid>
</Window>
