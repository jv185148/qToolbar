<Window x:Class="qControls.InputBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:qControls"
        mc:Ignorable="d"
        Title="InputBox" Height="110.534" Width="396" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded" WindowStyle="ToolWindow">
    <Grid>
        <TextBox Name="txtText" Height="23" Margin="10,17,12.6,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" KeyUp="txtText_KeyUp"/>
        <Button Name="btnCancel" Content="_Cancel" HorizontalAlignment="Left" VerticalAlignment="Top" Width="74" Margin="210,45,0,0" Height="26" Click="btnCancel_Click"/>
        <Button Name="btnAccept" Content="_Accept" Margin="302,45,12.6,0" Height="26" VerticalAlignment="Top" Click="btnAccept_Click"/>

    </Grid>
</Window>
